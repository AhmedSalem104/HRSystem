<script type="text/javascript">

           $(function () {
             $('#StartDateH').calendarsPicker({
                calendar: $.calendars.instance('ummalqura', 'ar'), showTrigger: '#calImg',
                onSelect: function () {

                    var date = $('#StartDateH').val();
                    var dateFormat = moment(date, 'iYYYY/iM/iD');
                    var toGregoriandate = dateFormat.format('YYYY/MM/D');

                    $('#StartDate').val(toGregoriandate);
                     $.ajax({
                        type: "POST",
                        cache: false,
                        url: '@Url.Action("AddDaysPublishDate", "DecisionEndStopHand")',
                         data: { days: $("#Period").val(), date: $('#StartDate').val() },
                        dataType: "json",
                        success: function (result) {

                            $("#PublishDate").val(result);
                            var date = new Date(result);
                            var day = date.getDate();
                            var month = date.getMonth() + 1;
                            var year = date.getFullYear();
                            if (month < 10) month = "0" + month;
                            if (day < 10) day = "0" + day;
                            var today = year + "-" + month + "-" + day;
                            $("#PublishDate").attr("value", today);

                            var date = $('#PublishDate').val();
                            var dateFormat = moment(date).format('DD/MM/YYYY')
                            var tohijridate = HijriJS.toHijri(dateFormat, "/");
                            $('#PublishDateH').val(tohijridate);

                        }
                    });

                }

            });

           });

           $("#Period").keyup(function () {
                $.ajax({
            type: "POST",
            //contentType: "application/json; charset=utf-8",
            cache: false,
            url: '@Url.Action("AddDaysPublishDate", "DecisionEndStopHand")',
                    data: { days: parseInt($(this).val()), date: $("#StartDate").val()},
             dataType: "json",
            success: function (result) {

                $("#PublishDate").val(result);
                /* alert($("#PublishDate").val())*/
                var date = new Date(result);


                var day = date.getDate();
                var month = date.getMonth() + 1;
                var year = date.getFullYear();

                if (month < 10) month = "0" + month;
                if (day < 10) day = "0" + day;

                var today = year + "-" + month + "-" + day;
                $("#PublishDate").attr("value", today);


                var datee = $('#PublishDate').val();
                var dateFormat = moment(datee).format('DD/MM/YYYY')

                var tohijridate = HijriJS.toHijri(dateFormat, "/");

                $('#PublishDateH').val(tohijridate);
            }
        });
           });

