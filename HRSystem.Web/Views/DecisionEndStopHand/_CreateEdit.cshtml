        <input id="SalaryStartDatee" hidden value="@Model.SalaryStartDate.Value.Date">

    }
    else
    {

        <input id="SalaryStartDatee" hidden value="1">

    }


    if (Model.EmpId != null)
    {
        <input id="EmpIdd" hidden value="@Model.EmpId">

    }
    else
    {

        <input id="EmpIdd" hidden value="1">

    }


    var V = Session["Value"];
}





